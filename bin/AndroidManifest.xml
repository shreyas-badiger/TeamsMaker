<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamsmaker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.teamsmaker.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.teamsmaker.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.teamsmaker.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.teamsmaker.page1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.teamsmaker.PAGE1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.teamsmaker.page2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.teamsmaker.PAGE2" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.teamsmaker.Page11"
            android:label="@string/title_activity_page11" >
        </activity>
    </application>

</manifest>
